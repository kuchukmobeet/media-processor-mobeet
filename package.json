{
  "name": "media-processor",
  "version": "1.0.0",
  "main": "dist/app.js",
  "scripts": {
    "dev": "tsx src/app.ts",
    "build": "tsc -p .",
    "start": "node dist/app.js",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepare": "husky"
  },
  "keywords": [
    "media",
    "processing",
    "ffmpeg",
    "image",
    "video"
  ],
  "author": "",
  "license": "ISC",
  "description": "High-performance media processing service for images and videos with filters, stickers, and text overlays",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "execa": "^8.0.1",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "multer": "^2.0.2",
    "pino": "^9.7.0",
    "zod": "^4.0.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.1.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.32.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  },
  "lint-staged": {
    "*.{ts,js,json,md}": [
      "prettier --write"
    ]
  }
}
