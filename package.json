{
  "name": "media-processor",
  "version": "1.0.0",
  "main": "dist/app.js",
  "scripts": {
    "dev": "tsx src/app.ts",
    "build": "tsc -p .",
    "start": "node dist/app.js",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": ["media", "processing", "ffmpeg", "image", "video"],
  "author": "",
  "license": "ISC",
  "description": "High-performance media processing service for images and videos with filters, stickers, and text overlays",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "execa": "^9.6.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "multer": "^2.0.2",
    "pino": "^9.7.0",
    "zod": "^4.0.13"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/multer": "^2.0.0",
    "@types/node": "^24.1.0",
    "eslint": "^9.32.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
