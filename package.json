{
  "name": "mobeet-media-processor",
  "version": "1.0.0",
  "description": "A minimal Express.js TypeScript media processing API",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "concurrently \"npm run build:watch\" \"npm run start:dev\"",
    "start:dev": "nodemon dist/server.js",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "express",
    "typescript",
    "media-processing",
    "api",
    "nodejs"
  ],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@types/got": "^9.6.12",
    "cors": "^2.8.5",
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "got": "^14.4.7",
    "helmet": "^8.1.0",
    "mime-types": "^3.0.1",
    "reflect-metadata": "^0.2.2",
    "tsyringe": "^4.10.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/mime-types": "^3.0.1",
    "@types/node": "^24.3.0",
    "@types/winston": "^2.4.4",
    "concurrently": "^9.2.0",
    "nodemon": "^3.1.10",
    "typescript": "^5.9.2"
  }
}
